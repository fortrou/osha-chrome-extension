{
	"manifest_version": 2,
	"name": "Online-shkola",
	"description": "Запускаем плагин онлайн-школы 'Альтернатива'",
	"version": "1.0.0",
	"icons": {"128": "osha.png"},
	"browser_action": {
		"default_icon": "alternativa_logo.png",
		"default_popup": "popup.html"
	},
	"permissions": [ "tabs", "notifications", "storage", "contextMenus", "http://*/*", "https://*/*", "activeTab", "cookies" ]
}